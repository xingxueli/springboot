<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.company.project.dao.FileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.company.project.entity.File">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="bin_data" property="binData" />
        <result column="original_name" property="originalName" />
        <result column="path" property="path" />
        <result column="format" property="format" />
        <result column="size" property="size" />
        <result column="md5" property="md5" />
        <result column="key_name" property="keyName" />
        <result column="is_deleted" property="isDeleted" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, bin_data, original_name, path, format, size, md5, key_name, is_deleted, create_time, update_time
    </sql>

</mapper>
